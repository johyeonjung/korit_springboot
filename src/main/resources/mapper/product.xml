<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.springboot.mapper.ProductMapper">
    <insert id="insert">
        insert into product_db
        values(default, #{product_name}, #{size}, #{price})
    </insert>

    <select id="findAllName">
        select
        product_name
        from
        product_db
    </select>

</mapper>